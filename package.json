{
	"name": "final-score",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"core-js": "^3.6.4",
		"dotenv": "^8.2.0",
		"firebase": "^7.14.1",
		"vue": "^2.6.11",
		"vue-router": "^3.1.6",
		"vuefire": "^2.2.1",
		"vuetify": "^2.2.11",
		"vuex": "^3.1.3"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.2.0",
		"@vue/cli-plugin-eslint": "~4.2.0",
		"@vue/cli-service": "~4.2.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.8.0",
		"eslint-plugin-vue": "^6.1.2",
		"sass": "^1.19.0",
		"sass-loader": "^8.0.0",
		"vue-cli-plugin-vuetify": "~2.0.5",
		"vue-template-compiler": "^2.6.11",
		"vuetify-loader": "^1.3.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {
			"indent": [
				2,
				"tab",
				{
					"SwitchCase": 1
				}
			],
			"no-tabs": [
				1,
				{
					"allowIndentationTabs": true
				}
			],
			"comma-dangle": [
				2,
				"always-multiline"
			],
			"quotes": [
				2,
				"single"
			]
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
